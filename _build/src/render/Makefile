OCAMLDEP=ocamldep 
all :

	$(MAKE) native
	$(MAKE) byte
	$(MAKE) exe
exe : render.exe
tryall :
	$(MAKE) byte || $(MAKE) native || echo "compilation failed in native and in byte"
all_cmx : bsl_init.cmx  init_render_001.cmx 
all_cmo : bsl_init.cmo  init_render_001.cmo 
cmxa: all_cmx
	/usr/lib/opa/ocaml/bin/ocamlopt.opt -linkall -a bsl_init.cmx  init_render_001.cmx  -o l.cmxa
cma: all_cmo
	ocamlc.opt -linkall -a bsl_init.cmo  init_render_001.cmo  -o l.cma

# Default compilation
render.exe : all_cmx
	$(NATIVE_LINKING) $@ 2> compilation.log

# Native compilation
native : cmxa
	$(NATIVE_LINKING) render.exe 2> compilation.log
render.exe.native : cmxa
	$(NATIVE_LINKING) $@ 2> compilation.log

# Bytecode compilation
byte : cma
	$(BYTECODE_LINKING) render.exe 2> compilation.log
render.exe.byte : cma
	$(BYTECODE_LINKING) $@ 2> compilation.log

# COMPILATION RULES

-include .depends

camlp4 :
	$(CAMLP4O) bsl_init.ml -o bsl_init.ml
	$(CAMLP4O) init_render_001.ml -o init_render_001.ml

clean :
	rm -f *.cmx *.cmi *.cmo *.o

show_old_source :
	ls | grep -v -E "bsl_init.ml|init_render_001.ml"

clean_old_source :
	ls | grep -v -E "Makefile|bsl_init.ml|init_render_001.ml|*.qml|*.exe" | xargs rm -f
